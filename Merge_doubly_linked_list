#include<bits/stdc++.h>
using namespace std;
class Node{
    public:
    int data;
    Node* next;
    Node* prev;
    Node(int val)
    {
        data=val;
        next=prev=NULL;
    }
};
class List{
    public:
    Node* head;
    Node* tail;
    List()
    {
        head=tail=NULL;
    }
    void push_back(int val)//insert at end
    {
        Node* newNode=new Node(val);
        if(head==NULL)
        {
            head=tail=newNode;
            return;
        }
        newNode->prev=tail;
        newNode->next=NULL;
        tail->next=newNode;
        tail=newNode;
    }
    void merge(Node* head1,Node* head2)
    {
       Node* t1=head1;
       Node* t2=head2;
       while(t1!=NULL && t2!=NULL)//until one list ends
       {
        Node* next1=t1->next;//store next pointers
        Node* next2=t2->next;
        t1->next=t2;//link t1 to t2
        t2->prev=t1;
        if(next1!=NULL)
        {
            t2->next=next1;//link t2 to next1
            next1->prev=t2;
        }
        t1=next1;//move to next nodes
        t2=next2;
       }
    }
    void Print()
    {
        Node* temp=head;
        while(temp!=NULL)
        {
            cout<<temp->data<<" ";
            temp=temp->next;
        }
        cout<<endl;
    }
};
int main()
{
    List l1,l2;
    l1.push_back(1);
    l1.push_back(3);
    l1.push_back(5);
    l1.push_back(7);
    l2.push_back(2);
    l2.push_back(4);
    l2.push_back(6);
l2.push_back(8);
l1.Print();
l1.merge(l1.head,l2.head);
l1.Print();
return 0;
}
