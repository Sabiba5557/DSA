#include<bits/stdc++.h>
using namespace std;
void merge(int arr[],int lb, int mid, int ub)//lb=lower bound, ub=upper bound
{
    int i=lb;//starting index of left subarray
    int j=mid + 1;//starting index of right subarray
    int k=lb;
    int b[1000];  
    while(i<=mid && j<=ub)//merging two subarrays
    {
        if(arr[i]<=arr[j])//for stable sorting
            b[k++]=arr[i++];//copying element from left subarray
        else
            b[k++]=arr[j++];//copying element from right subarray
    }
    while(i<=mid)
        b[k++]=arr[i++];//copying remaining elements of left subarray
    while(j<=ub)
        b[k++]=arr[j++];//copying remaining elements of right subarray
    for(int x=lb;x<=ub;x++)//copying back to original array
        arr[x]=b[x];
}
void mergeSort(int arr[], int lb, int ub)//function for merge sort
{
    if(lb<ub)
    {
        int mid=(lb + ub)/2;
        mergeSort(arr,lb,mid);//sorting left subarray
        mergeSort(arr,mid+1,ub);//sorting right subarray
        merge(arr,lb,mid,ub);//merging sorted subarrays
    }
}
int main()
{
    int n;
    cin>>n;

    int arr[n];
    for(int i=0;i<n;i++)
        cin>>arr[i];
    mergeSort(arr,0,n-1);
    for(int i=0;i<n;i++)
        cout<<arr[i]<<" ";
    return 0;
}
