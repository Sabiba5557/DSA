#include<iostream>
using namespace std;
class Node{
    public:
    int data;
    Node* next;
    Node(int val){
        data=val;
        next=NULL;
    }
    };
    class List{
        Node* head;
        Node* tail;
        public:
        List(){
            head=tail=NULL;
        }
        void push_back(int val){
            Node* newNode=new Node(val);
            Node* temp=head;
          if(head==NULL){
              head=tail=newNode;
          }
          else{
              tail->next=newNode;
              tail=newNode;
          }
            }
            void Print(){
                Node* temp=head;
                while(temp!=NULL){
                    cout<<temp->data<<" ";
                    temp=temp->next;
                }
                cout<<endl;
            }
            int middle(){
                Node* slow=head;
                Node* fast=head;
                while(fast!=NULL && fast->next!=NULL){
                    slow=slow->next;
                    fast=fast->next->next;
                }
                return slow->data;
            }
        };
        int main(){
            List ll;
            int n,v;
            cin>>n;
            for(int i=0;i<n;i++){
                cin>>v;
                ll.push_back(v);
            }
            ll.Print();
            cout<<ll.middle()<<endl;
        }
