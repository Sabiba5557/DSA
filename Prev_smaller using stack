#include<bits/stdc++.h>
using namespace std;
vector<int>prevsmall(vector<int>&a)
{
    vector<int>ans(a.size(),-1);
    stack<int>st;
    for(int i=0;i<a.size();i++)
    {
        while(!st.empty() && st.top()>a[i])
        {
            st.pop();
        }
        if(!st.empty())
        {
            ans[i]=st.top();
        }
        st.push(a[i]);
    }
    return ans;
}
int main()
{
    vector<int>arr={4,5,2,10,8};
    vector<int>res=prevsmall(arr);
    for(int x:res)
    {
        cout<<x<<" ";
    }
    cout<<endl;
    return 0;
}
