#include<bits/stdc++.h>
using namespace std;
bool isValid(string s)
{
    stack<char>st;
    for(char c:s)
    {
        if(c=='(' || c=='{'|| c=='[')//opening brackets
        {
            st.push(c);//push into stack
        }
        else{
        if (st.empty())//if stack is empty and closing bracket found
                return false;

                char top = st.top();//get the top element
                st.pop();//pop the top element
                if((c==')' && top!='(') || (c=='}' && top!='{') || (c==']' && top!='['))
                    return false;//mismatched brackets
            }
        }
        return st.empty();//if stack is empty at the end, brackets are balanced
    }
    
int main()
{
    string s;
    cin>>s;
    if(isValid(s))
    {
        cout<<"Valid"<<endl;
    }
    else{
        cout<<"Invalid"<<endl;
    }
}
