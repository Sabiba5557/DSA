#include<iostream>
using namespace std;
class Node{
    public:
    int data;
    Node* next;
    Node(int val)
    {
        data=val;
        next=NULL;
    }
};
class List{
    public:
    Node* head;
    Node* tail;
    List()
    {
        head=tail=NULL;
    }
    void push_back(int val)
    {
        Node* newNode=new Node(val);
        if(head==NULL){
            head=tail=newNode;
        }
        else
        {
            tail->next=newNode;
            tail=newNode;
            tail->next=NULL;
        }
    }
    //when sorted
    Node* Merge_sort(Node* a,Node* b){//head of 2 List
    if(a==NULL) return b;
    if(b==NULL) return a;
    Node* result=NULL;
    if(a->data<=b->data){
        result=a;
        result->next=Merge_sort(a->next,b);
    }
    else
{
    result=b;
    result->next=Merge_sort(a,b->next);
}
return result;
}
void Print()
{
    Node* temp=head;
    while(temp!=NULL)
    {
        cout<<temp->data<<" ";
        temp=temp->next;
    }
}
};
int main()
{
    List ll,l2;
    int n,k,s;
    cin>>n>>s;
    for(int i=0;i<n;i++)
    {
        cin>>k;
        ll.push_back(k);
    }
    for(int j=0;j<s;j++)
    {
        cin>>k;
        l2.push_back(k);
    }
    ll.head=ll.Merge_sort(ll.head,l2.head);
    ll.Print();
}
